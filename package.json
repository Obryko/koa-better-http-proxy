  "description": "http proxy middleware for koa",
  "engines": {
    "node": ">=10.0.0"
  },
  "engineStrict": true,
  "main": "index.js",
  "scripts": {
    "test": "npm -s run mocha && npm run -s lint && npm run -s jscs",
    "test:debug": "mocha debug -R spec test --recursive",
    "mocha": "mocha -R spec test --recursive",
    "lint": "jshint index.js test/*.js test/**/*js lib/*js app/**/*js ",
    "jscs": "eslint index.js test/*.js test/**/*js lib/*js app/**/*js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nsimmons/koa-better-http-proxy.git"
  },
  "types": "types.d.ts",
  "keywords": [
    "koa-better-http-proxy"
  ],
  "author": {
    "name": "nsimmons",
    "email": "nick.simmons.514@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nsimmons/koa-better-http-proxy/issues"
  },
  "devDependencies": {
    "body-parser": "^1.20.0",
    "chai": "^4.3.6",
    "cookie-parser": "^1.4.6",
    "eslint": "^8.13.0",
    "eslint-plugin-import": "^2.26.0",
    "express": "^4.17.3",
    "jshint": "^2.13.4",
    "koa": "^2.13.4",
    "mocha": "^9.2.2",
    "supertest": "^6.2.2"
  },
  "dependencies": {
    "es6-promise": "^4.2.8",
    "raw-body": "^2.5.1",
    "winston": "^3.7.2"
  },
